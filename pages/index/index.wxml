<list wx:if="{{PageCur=='list'}}"></list>
<upload wx:if="{{PageCur=='upload'}}"></upload>
<about wx:if="{{PageCur=='about'}}"></about>
<chart wx:if="{{PageCur=='chart'}}"></chart>


<view class="cu-bar tabbar bg-white shadow foot">
  <view class="action" bindtap="NavChange" data-cur="list">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/basics{{PageCur=='list'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='list'?'text-blue':'text-gray'}}">明细</view>
  </view>





  <view class="action" bindtap="NavChange" data-cur="upload">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/component{{PageCur=='upload'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='upload'?'text-blue':'text-gray'}}">记账</view>
  </view>




  <view class="action" bindtap="NavChange" data-cur="chart">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/plugin{{PageCur=='chart'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='chart'?'text-blue':'text-gray'}}">图表</view>
  </view>

  
  <view class="action" bindtap="NavChange" data-cur="about">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/about{{PageCur=='about'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='about'?'text-blue':'text-gray'}}">关于</view>
  </view>
</view>